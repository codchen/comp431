{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Social App Frontend",
  "main": "./src/index.js",
  "scripts": {
    "clean": "rimraf dist/bundle.js*",
    "lint": "eslint src --ext .js --ext .jsx --cache",
    "lint:test": "eslint tests --ext .js --ext .jsx --cache",
    "lint:e2e": "eslint e2e --ext .js --ext .jsx --cache",
    "watch": "webpack --watch",
    "build": "webpack -p",
    "deploy": "webpack -p && surge -p dist",
    "start": "serve -p 8080 dist",
    "dev": "webpack-dev-server --content-base dist --inline -d",
    "test": "mocha --opts mocha.opts tests/*.spec.js tests/*/*.spec.js",
    "test:watch": "npm run test -- -w",
    "karma": "karma start --single-run",
    "karma:watch": "karma start",
    "e2e": "npm run e2e:base -- --reporter spec",
    "e2e:base": "mocha --opts e2e/mocha.opts e2e/*.spec.js",
    "e2e:watch": "npm run e2e:base -- --reporter spec -w",
    "e2e:xunit": "npm run e2e:base -- --reporter xunit > e2e/results.xml"
  },
  "author": "Tony Chen",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.2",
    "eslint": "^3.17.0",
    "eslint-plugin-babel": "^4.1.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.10.1",
    "image-webpack-loader": "^3.2.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jsdom": "^9.11.0",
    "jsdom-global": "^2.1.1",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^0.2.3",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "mock-fetch": "https://www.clear.rice.edu/comp431/sample/mock-fetch.tgz",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0",
    "selenium-webdriver": "^3.3.0",
    "semantic-ui-react": "^0.67.0",
    "serve": "^5.0.3",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  }
}
